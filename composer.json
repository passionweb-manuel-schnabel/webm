{
  "name": "passionweb/webm",
  "license": "GPL-3.0",
  "description": "Creates a WebM file for every configured (and supported) video format. Either via Symfony command or hook. (TYPO3 CMS)",
  "type": "typo3-cms-extension",
  "homepage": "https://www.passionweb.de",
  "version": "2.1.0",
  "authors": [
    {
      "name": "Manuel Schnabel",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "typo3/cms-core": "^12.4",
    "php-ffmpeg/php-ffmpeg": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Passionweb\\Webm\\": "Classes/"
    }
  },
  "replace": {
      "typo3-ter/webm": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "webm"
    }
  }
}
